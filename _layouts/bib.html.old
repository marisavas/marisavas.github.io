<div style="width:1130px;">
{% if entry.series %}
<div style="float:left;position:relative;width:200px;text-align:left;font-size:16px;padding-top:10px;padding-left:80px;">[{{entry.series}}]</div>
<div style="float:left;position:relative;width:930px;text-align:left;font-size: 16px;padding-left:20px;line-height:20px;">{{reference}}</div>
</div>
{% else %}
<div style="float:left;position:relative;width:200px;text-align:left;font-size:16px;padding-top:20px;padding-left:80px;"> [{{entry.howpublished}}] </div>
<div style="float:left;position:relative;width:930px;text-align:left;font-size:16px;padding-left:20px;line-height:20px;">
<!-- {% assign currentdate = entry.date | date: "%B %Y" %} -->
{{entry.date}}
{% for author in entry.author_array %}
{{ author.first }} {{ author.last }}{% unless forloop.last %},{% endunless %}{% if forloop.last %}.{% endif%}
{% endfor %}<b>{{ entry.title }}</b>. {{ entry.month_string }} {{ entry.year }}, {{entry.note}}.
</div>

</div>
{% endif %}



